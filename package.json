{
  "name": "notion-upload",
  "version": "1.0.0",
  "description": "Sync Notion pages to Supabase database with image upload to Cloudflare",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "sync": "npm run build && node dist/index.js sync",
    "export": "npm run build && node dist/index.js export",
    "check-notion": "npm run build && node dist/index.js check-notion",
    "check-db": "npm run build && node dist/index.js check-db",
    "server": "npm run build && node dist/server.js",
    "server:dev": "ts-node src/server.ts"
  },
  "keywords": [
    "notion",
    "supabase",
    "cloudflare",
    "sync",
    "markdown"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.893.0",
    "@notionhq/client": "^2.2.15",
    "@supabase/supabase-js": "^2.39.0",
    "@types/node-fetch": "^2.6.11",
    "commander": "^11.1.0",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "node-fetch": "^2.7.0",
    "notion-to-md": "^3.1.9",
    "sharp": "^0.34.4",
    "turndown": "^7.1.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/turndown": "^5.0.4",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  }
}
